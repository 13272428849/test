#include <iostream>
#include <cstdio>

using namespace std;
const int N =1e6+10;
int q[N],n,t[N];
void mergepx(int q[],int l,int r)
{
    int mid=(l+r)>>1;

    if(l>=r) return;  
    mergepx(q,l,mid);
    mergepx(q,mid+1,r);


    int i=l,j=mid+1; 
    int k=0;
    while(i<=mid&&j<=r)
        if(q[i]<=q[j]) t[k++] = q[i++]; 
    else t[k++] = q[j++];
    while(i<=mid) t[k++] = q[i++]; 
    while(j<=r) t[k++] = q[j++];
    for(int i=l,j=0;i<=r;i++,j++) q[i]=t[j]; //递归的末尾
}
int main()
{
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        scanf("%d",&q[i]);
    mergepx(q,0,n-1);
    for(int i=0;i<n;i++)
        printf("%d ",q[i]);
    return 0;
}

